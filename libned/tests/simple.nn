def add<fty fp, int N>(fp<N> lhs, fp<N> rhs) -> sum:
	fp<N> out
	out.forward = add(lhs.forward, rhs.forward)
	lhs.backward = out.backward
	rhs.backward = out.backward
	return out

def model<fty fp, int N>(fp<N> lhs, fp<N> rhs) -> out:
	return add(lhs, rhs)
