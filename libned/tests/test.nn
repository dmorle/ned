intr add<fwidth fw, int *shape>(tensor<fw, *shape> t1, tensor<fw, *shape> t2):
    tensor<fw, *shape> out
    return out

def xavier<fwidth fw, int *shape>():
    tensor<fw, *shape> w
    return w

def model<int N>(tensor<f32, N> inp):
    tensor<f32, 2 * N> out1 = xavier<f32, 2 * N>()
    tensor out2 = xavier<f32, N, N>()
    out2 = add(out2, out2)
    return out1, out2
